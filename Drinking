local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "BaconBoss Hub | Drinking Legends!", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


getgenv().tpPlace = "string"

function Teleport(teleportPlace)
    local player = game.Players.LocalPlayer
    player.Character.HumanoidRootPart.CFrame = teleportPlace
end


local TpTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tp2Tab = Window:MakeTab({
	Name = "Egg",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tp3Tab = Window:MakeTab({
	Name = "Tp",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tp4Tab = Window:MakeTab({
	Name = "Others",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

_G.click = true
_G.sell = true
function click()
     while _G.click == true do
        game:GetService("VirtualUser"):ClickButton1(Vector2.new(9e9, 9e9))
     wait(0)
    end
end

function sell()
    while _G.sell == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Common"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PlayerService"):WaitForChild("RF"):WaitForChild("Sell"):InvokeServer()
        wait(0)
   end
end

TpTab:AddToggle({
	Name = "Auto Sip",
	Default = false,
	Callback = function(Value)
		_G.click = Value
        click()
	end    
})
 
TpTab:AddToggle({
	Name = "Auto Sell",
	Default = false,
	Callback = function(Value)
		_G.sell = Value
        sell()
	end    
})
 
_G.coin = true
function coin()
     while _G.coin == true do
        for i, part in pairs(game.Workspace.CoinLocations:GetChildren()) do
            if part:IsA("BasePart") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part.CFrame
                wait(0.1)
            end
         task.wait()
        end
    end
end

TpTab:AddToggle({
	Name = "Auto Collect Coin OP",
	Default = false,
	Callback = function(Value)
		_G.coin = Value
        coin()
	end    
})

TpTab:AddLabel("")

_G.buy2 = true
_G.rb = true

function buy2()
    while _G.buy2 == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Common"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ShopService"):WaitForChild("RF"):WaitForChild("UpgradeBackpack"):InvokeServer()
        wait(0)
   end
end

function rb()
    while _G.rb == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Common"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ShopService"):WaitForChild("RF"):WaitForChild("Rebirth"):InvokeServer()
        wait(0)
   end
end

