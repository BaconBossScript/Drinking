Tp3Tab:AddDropdown({
	Name = "Select",
	Default = "Spawn",
	Options = {"Spawn", "Autumn", "CottonCandy", "Mint", "Desert", "Ocean", "Snow", "Lava", "Angel"},
	Callback = function(Value)
	    getgenv().tpPlace = Value  
	end    
})

Tp3Tab:AddButton({
	Name = "Tp",
	Callback = function()
	    if getgenv().tpPlace == "Spawn" then
      	 Teleport(game:GetService("Workspace").RegionMultipliers.Spawn.CFrame)
		elseif getgenv().tpPlace == "Autumn" then
		 Teleport(game:GetService("Workspace").RegionMultipliers.Autumn.CFrame)
        elseif getgenv().tpPlace == "CottonCandy" then
		 Teleport(game:GetService("Workspace").RegionMultipliers.CottonCandy.CFrame)
        elseif getgenv().tpPlace == "Mint" then
		 Teleport(game:GetService("Workspace").RegionMultipliers.Mint.CFrame)
        elseif getgenv().tpPlace == "Desert" then
		 Teleport(game:GetService("Workspace").RegionMultipliers.Desert.CFrame)
        elseif getgenv().tpPlace == "Ocean" then
         Teleport(game:GetService("Workspace").RegionMultipliers.Ocean.CFrame)
        elseif getgenv().tpPlace == "Snow" then
         Teleport(game:GetService("Workspace").RegionMultipliers.Snow.CFrame)
        elseif getgenv().tpPlace == "Lava" then
         Teleport(game:GetService("Workspace").RegionMultipliers.Lava.CFrame)
        elseif getgenv().tpPlace == "Angel" then
         Teleport(game:GetService("Workspace").RegionMultipliers.Angel.CFrame)
		end
    end
}) 

_G.segg = "Classic"
_G.egg = true
function egg()
     while _G.egg == true do    
        game:GetService("ReplicatedStorage"):WaitForChild("Common"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("HatchService"):WaitForChild("RF"):WaitForChild("Hatch"):InvokeServer(_G.segg, 1)
        wait(0)
    end
end

Tp2Tab:AddToggle({
	Name = "Auto Hatch",
	Default = false,
	Callback = function(Value)
		_G.egg = Value
        egg()
	end    
})

